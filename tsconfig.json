{
  "include": ["src/*.ts"],
  "exclude": [],
  "compilerOptions": {
    "noEmitOnError": false,
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "target": "ES2016",
    "lib": ["DOM", "ES2016"]
  }
}
